{
  "name": "facegram",
  "version": "1.0.0",
  "description": "A simple and extensible bridge",
  "main": "/dist/index",
  "typings": "/src/types/index.d.ts",
  "scripts": {
    "start": "npm run serve",
    "dev": "npm run tslint && ts-node src/index.ts",
    "build": "npm run tslint && rm -rf dist && npm run build-ts && pkg . --outdir bin",
    "serve": "node dist/index.js",
    "test": "npm run tslint && rm -rf dist && npm run build-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feelfreelinux/ChatPlug.git"
  },
  "keywords": [
    "facebook",
    "telegram",
    "facebook-api",
    "telegram-api",
    "chat",
    "bridge"
  ],
  "author": "feelfreelinux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feelfreelinux/ChatPlug/issues"
  },
  "homepage": "https://github.com/feelfreelinux/ChatPlug#readme",
  "dependencies": {
    "discord.js": "^11.3.2",
    "facebook-chat-api": "^1.6.0",
    "google-protobuf": "^3.6.0",
    "grpc": "^1.12.4",
    "node-telegram-bot-api": "^0.30.0",
    "npmlog": "^4.1.2",
    "rxjs": "^6.2.1",
    "tdl": "^3.8.1",
    "@iarna/toml": "^1.7.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "@types/node-telegram-bot-api": "^0.30.1",
    "@types/npmlog": "^4.1.0",
    "pkg": "^4.3.1",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "bin": "dist/index.js",
  "pkg": {
    "scripts": "dist/**/*.js"
  }
}
